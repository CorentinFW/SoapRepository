  <wsdl:message name="rechercherChambresRequest">
    <wsdl:part element="tns:rechercherChambresRequest" name="rechercherChambresRequest">
    </wsdl:part>
  </wsdl:message>
  <wsdl:portType name="HotelPort">
    <wsdl:operation name="effectuerReservation">
      <wsdl:input message="tns:effectuerReservationRequest" name="effectuerReservationRequest">
    </wsdl:input>
