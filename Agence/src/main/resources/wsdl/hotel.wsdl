    <wsdl:operation name="getHotelInfo">
      <wsdl:input message="tns:getHotelInfoRequest" name="getHotelInfoRequest">
    </wsdl:input>
      <wsdl:output message="tns:getHotelInfoResponse" name="getHotelInfoResponse">
    </wsdl:output>
    </wsdl:operation>
    <wsdl:operation name="getReservations">
      <wsdl:input message="tns:getReservationsRequest" name="getReservationsRequest">
    </wsdl:input>
      <wsdl:output message="tns:getReservationsResponse" name="getReservationsResponse">
    </wsdl:output>
    </wsdl:operation>
